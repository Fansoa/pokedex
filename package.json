  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite -c ./configs/vite/vite.config.js",
    "build": "vite build -c ./configs/vite/vite.config.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
