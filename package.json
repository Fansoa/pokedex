    "build-storybook": "storybook build -c ./configs/storybook",
    "test": "vitest -c ./configs/vite.config.js",
    "coverage": "vitest run --coverage -c ./configs/vite/vite.config.js",
    "scanner": "i18next-scanner --config ./services/i18next/scanner/index.cjs"
  },
  "dependencies": {
    "@emotion/jest": "^11.11.0",
