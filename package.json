    "@storybook/react": "^8.0.6",
    "@storybook/react-vite": "^8.0.6",
    "@storybook/test": "^8.0.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint-plugin-storybook": "^0.8.0",
    "i18next-scanner": "^4.4.0",
    "storybook": "^8.0.6",
    "vite": "^5.2.0"
